<FirstWindow>:
    name: "first"
    row: 3
    BoxLayout:
        orientation: 'vertical'
        size: root.width, root.height
        padding: 5
        spacing: 2
        Image:
            source: 'source/cat_doc.png'
            size_hint: (1, 3)
            allow_stretch: True
            keep_ratio: True
        Label:
            id: FirstLabel
            text: 'Выберите опцию'
            font_size: '22sp'
            font_name: "source/Century Gothic.ttf"
            color: '#FF8C00'
            halign: 'center'

        RoundedButton:
            id: ButtonCheck
            text: 'Проверка документов'
            font_size: '20sp'
            font_name: "source/Century Gothic.ttf"
            pos_hint: {'center_x': 0.5}
            size_hint: (1, .3)

            on_release: root.calc()

        RoundedButton:
            id:ButtonList
            text: 'Лист с документами'
            font_size: '20sp'
            font_name: "source/Century Gothic.ttf"
            pos_hint: {'center_x': 0.5}
            size_hint: (1, .3)
            on_release:
                app.root.current = "second"
                root.manager.transition.direction = "left"
<RoundedButton@Button>
    id: Rounded
    background_color: (219/255, 113/255, 26/255, 1)
    background_normal:''
    canvas.before:

